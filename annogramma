let arr1 = "aabbsyaoi";
let arr2 = "aabbsyaio";
let mass1=arr1.split('');
let mass2=arr2.split('');
let a=0;
let result1=(mass1.sort());
let result2=(mass2.sort());

if(arr1.length==arr2.length){
for(let i=0;i<result1.length;i++){
if (result1[i]==result2[i]){
a++
}
}
if (a == result1.length){
  console.log("We have anagramma");
}
}
